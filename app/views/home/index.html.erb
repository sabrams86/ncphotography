<section class="main-section paddind" id="Portfolio"><!--main-section-start-->
	<% @albums.each do |album| %>
		<button class="btn btn-success" type="button" data-toggle="collapse" data-target="#<%=album.name%>" aria-expanded="false" aria-controls="<%=album.name%>"><%= album.name %></button>
	<% end %>
	<% @albums.each do |album| %>
		<div class="collapse" id="<%=album.name%>"></div>
			<div class="well">
				<table class="table table-striped">
					<thead>
						<tr>
							<th>Photo</th>
							<th>Name</th>
							<th>Album</th>
							<th>Options</th>
						</tr>
					</thead>
					<tbody>
					<% album.photos.each do |photo| %>
						<tr>
							<td>
								<%= link_to image_tag(photo.image.thumb('180x180#').url, alt: photo.title, class: 'img-thumbnail'), photo.image.url, target: '_blank'%>
							</td>
							<td>
								<%= photo.title%>
							</td>
							<td>
								<%= album.name%>
							</td>
					<% end %>
					</tbody>
				</table>
			</div>
		</div>
	<% end %>



